<%= form_with url: search_path, method: :get, local: true do |f| %>
  <%= f.text_field :search %>
  <%= f.select :model, [["Book", "book"], ["User", "user"]] %>
  <%= f.select :method, [["完全一致", "perfect"], ["前方一致", "forward"], ["後方一致", "backword"], ["部分一致", "partial"]] %>
  <%= f.submit "Search", class: "btn btn-sm btn-light" %>
<% end %>